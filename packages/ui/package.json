{
  "name": "@taxonomy/ui",
  "version": "0.0.0",
  "description": "ui",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ap0nia/taxonomy",
    "directory": "packages/ui"
  },
  "keywords": [],
  "scripts": {
    "postinstall": "svelte-kit sync",
    "format": "prettier --write .",
    "lint": "eslint .",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "check": "svelte-check"
  },
  "type": "module",
  "exports": {
    "./*": {
      "types": "./src/lib/components/ui/*",
      "svelte": "./src/lib/components/ui/*",
      "import": "./src/lib/components/ui/*"
    },
    "./aceternity/*": {
      "types": "./src/lib/components/aceternity/*",
      "svelte": "./src/lib/components/aceternity/*",
      "import": "./src/lib/components/aceternity/*"
    },
    "./tailwind.config": {
      "types": "./tailwind.config.d.ts",
      "import": "./tailwind.config.js",
      "default": "./tailwind.config.js"
    }
  },
  "typesVersions": {
    "*": {
      "./*": [
        "./src/lib/components/ui/*"
      ],
      "./aceternity/*": [
        "./src/lib/components/aceternity/*"
      ],
      "./tailwind.config": [
        "./tailwind.config.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@iconify/json": "^2.2.200",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tailwindcss/typography": "^0.5.12",
    "@taxonomy/tsconfig": "workspace:^",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.1",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3",
    "svelte": "^4.2.13",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "typescript-svelte-plugin": "^0.3.37",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.2.8"
  },
  "dependencies": {
    "bits-ui": "^0.21.3",
    "clsx": "^2.1.0",
    "lucide-svelte": "^0.368.0",
    "tailwind-merge": "^2.2.2"
  }
}
